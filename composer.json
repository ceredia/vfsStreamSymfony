{
    "name": "ceredia/vfs-stream-symfony",
    "description": "Extends vfsStream's virtual file system to comfortably implement functional tests for file uploads using Symfony's UploadedFile class.",
    "type": "library",
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/http-foundation": "^3.0",
        "mikey179/vfsStream": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Robin Schrage",
            "email": "r.schrage@ceredia.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": { "ceredia\\vfsStreamSymfony\\": "src/" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
